.ul2 {
	list-style: none;
	padding-left: 10px;
	animation: cambiar 7s infinite;
}

.ul2{
	margin: 0;
}

@keyframes cambiar {
	
	0%{ margin-top: 0;}
	20%{ margin-top: 0;}
	
	25% {margin-top: -40px;}
	50% {margin-top: -40px;}
	
	55% {margin-top: -80px;}
	80% {margin-top: -80px;}
	
	85% {margin-top: -60px;}
	95% {margin-top: -60px;}
	
	95% {margin-top: 0;}
}

