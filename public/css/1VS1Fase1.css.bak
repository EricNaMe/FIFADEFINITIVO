#menuLateral 
{     
    z-index: 8;
    background-color: green;
    position:fixed; 
    float: left;
    height: 92%;
    width: 18%;
    left:0px; 
    top:10%;
    list-style-type: none;

}




 ul#ListaMenuLateral {
     margin-top: 50%;
     text-align: center;
     padding: 0;
     
     font-family: sans-serif;
     font-size: 16px;
     font-weight: bold;
    }
    
     ul#ListaLateral li {
	
	float: left;
        
       
	height:2.5em;
	line-height:2.5em;

	
  }

  ul#ListaMenuLateral a {
        
	color: black;
	text-decoration:none; 
	padding: 20px 0; 
	display: block;
	height: 100%;
}




#ListaMenuLateral li:hover {
   
       background-color: whitesmoke;
     }
     
     
   #ListaMenuLateral li:hover a {
       
       color: black;
     }
     
     
     
     #ListaMenuLateral li a.active{
         background: #999; 
     }
     
 #ListaMenuLateral li a:hover  {
    background: #999; 
   
    color: #fff;
}
#ListaMenuLateral li ul {
    padding: 0px;
    list-style-type: none;
    display: none; // used to hide sub-menus
}
#ListaMenuLateral li ul li a {
  
    padding: 10px 35px;
    background: whitesmoke;
    border-bottom: 1px dotted #ccc;
}    

     
